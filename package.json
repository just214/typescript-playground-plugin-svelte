{
  "name": "typescript-playground-plugin-svelte",
  "description": "Easily create TypeScript Playground Plugins with Svelte.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.js;",
    "compile": "tsc",
    "start": "concurrently -p \"[{name}]\" -n \"ROLLUP,SITE\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn rollup -c rollup.config.js --watch\" \"yarn serve dist\"",
    "prepublish": "yarn build",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "concurrently": "^5.1.0",
    "monaco-editor": "^0.20.0",
    "node-fetch": "^2.6.0",
    "rollup": "^1.32.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "serve": "^11.3.0",
    "svelte": "^3.19.1",
    "typescript": "latest"
  },
  "dependencies": {
    "tslib": "^1.11.1"
  }
}
